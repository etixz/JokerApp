<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main_nav"
    app:startDestination="@id/startFragment">
    <fragment
        android:id="@+id/startFragment"
        android:name="eb.egonb.jokerapp.fragments.StartFragment"
        android:label="fragment_start"
        tools:layout="@layout/fragment_start" >
        <action
            android:id="@+id/start_to_jokelist"
            app:destination="@id/jokeListFragment2"
            app:enterAnim="@anim/nav_default_enter_anim" />
    </fragment>
    <fragment
        android:id="@+id/jokeListFragment2"
        android:name="eb.egonb.jokerapp.fragments.JokeListFragment"
        android:label="fragment_joke_list"
        tools:layout="@layout/fragment_joke_list" />
</navigation>